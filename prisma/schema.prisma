generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}
model User {
  id       Int     @id @default(autoincrement())
  username String @unique
  password String
}
model Consulta {
  id     Int      @id @default(autoincrement()) 
  nombre String
  apellido String
  asunto String
  mail   String
  fecha  DateTime @default(now())               
}
