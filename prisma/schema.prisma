generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int     @id @default(autoincrement())
  username String
  password String
}
model Consulta{
  id Int @id @default(autoincrement())
  contacto String
  mensaje String
  fecha DateTime @default(now())
}
