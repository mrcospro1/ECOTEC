generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}

model Consulta {
  id       Int      @id @default(autoincrement())
  nombre   String
  apellido String
  asunto   String
  mail     String
  fecha    DateTime @default(now())
}
